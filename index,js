//8. let function fullName 
function fullName (firstName, lastName) {
    return(firstname + " " + lastName);
}
console.log (fullName)
//9. function to determine if sum of array > 100
function hundred (array) {
    sum = 0;
    for (i = 0; i < array.length; i++) {
        sum = sum + array[i];
    }
    if (sum > 100) {
        return true;
    }
    else {
        return false;
    }
}
console.log(hundred(ages));
//10. write a function that takes an array of numbers and returns the average of the elements in the array
const average = array => array.length((a, b) => a + b) / array.length;
console.log(average(1,18, 47, 22, 34));
